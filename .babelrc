{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "env": {
    "test": {
      "presets":[
        ["es2015"],
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "build": {
      "plugins": [
        "babel-plugin-transform-import-extension-jsx-to-js"
      ]
    },
    "coverage": {
      "plugins": [
        [
          "istanbul", {
            "exclude": [
              "src/**/*.spec.jsx",
              "**/TestUtil.js"
            ]
          }
        ]
      ]
    }
  },
  "plugins": [
    [
      "import", {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}
